<script>
const stripe = Stripe('pk_live_51RyfKJJVP4NqYsSPwkYp7GT3N5N63B42u8tNMK4PwxGLoIeeQJSgdeyyFaYKbf3k1lNkc078SbWTFnJpP4eY3oY800uZkbsAC5');
document.querySelectorAll('.stripe-btn').forEach(btn => {
    btn.addEventListener('click', async () => {
        const price = parseInt(btn.dataset.price) * 100;
        const {error} = await stripe.redirectToCheckout({
            line_items: [{price_data: {currency: 'cad', product_data: {name: `CopyClone ${btn.dataset.price}`}, unit_amount: price}, quantity: 1}],
            mode: 'payment',
            success_url: window.location.href + '?success=true',
            cancel_url: window.location.href
        });
        if (error) alert(error.message);
    });
});
document.querySelectorAll('a[href^="#"]').forEach(a=>a.addEventListener('click',e=>{e.preventDefault();document.querySelector(a.getAttribute('href')).scrollIntoView({behavior:'smooth'});}));
if (window.location.search.includes('success')) alert('âœ… Payment successful! Check email for clone instructions.');
